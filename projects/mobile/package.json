{
  "name": "@smoex-master/mobile",
  "version": "0.1.0",
  "homepage": "https://learn.smoex.com",
  "proxy": "https://admin.smoex.com",
  "private": true,
  "scripts": {
    "start": "craco start",
    "test": "craco test",
    "eject": "craco eject",
    "build": "craco build",
    "build:dev": "PUBLIC_URL=/ craco build",
    "build:prod": "NODE_ENV=production craco build && npm run upload:prod",
    "upload:prod": "smoex-scripts upload ./build",
    "format": "npm run format:tslint && npm run format:prettier",
    "format:tslint": " tslint --fix --config ./tslint.json 'src/**/*.{ts,tsx}'",
    "format:stylelint": "stylelint --fix  --syntax scss 'src/**/*.*css'",
    "format:prettier": "prettier --config .prettierrc --write 'src/**/*.{js,jsx,ts,tsx,scss}'",
    "analyze": "cross-env ANALYZE=true npm run build"
  },
  "dependencies": {
    "@react-kits/dom": "^0.0.4",
    "@react-kits/redux": "^0.0.4",
    "@smoex-logic/basic": "^0.0.2",
    "@smoex-logic/user": "^0.0.2",
    "@smoex-master/logic": "^0.0.1",
    "@smoex-mobile/basic": "^0.0.3",
    "@types/vfile-message": "^2.0.0",
    "axios": "^0.19.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@craco/craco": "^5.5.0",
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.7.8",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.4",
    "@types/react-dom": "^16.9.1",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.3",
    "babel-loader": "^8.0.6",
    "node-sass": "^4.12.0",
    "react-scripts": "^3.1.2",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.11.1",
    "tslint": "^5.20.0",
    "tslint-react": "^4.1.0",
    "typescript": "3.6.3",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

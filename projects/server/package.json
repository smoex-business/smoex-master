    "koa2-proxy-middleware": "^0.0.4",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
